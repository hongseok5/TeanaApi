    "aggs" : {
        "keyword_over_time" : {
            "terms": {
                "script": {
                  "lang": "painless",
                  "source": "doc['start_time'].value.hourOfDay"
                }

            },
            "aggs" : {
              "aggs_keyword" : {
                "nested" : {
                  "path" : "neutral_word"
                },
                "aggs" : {
                  "top_keyword" : {
                    "terms" : {
                      "field": "neutral_word.word",
                      "size" : 15
                    }
                  }
                }
              }
            }
        }
    }
}

// GET /call_test3/_search?size=0
{
    "aggs" : {
        "keyword_over_time" : {
            "terms": {
                "script": {
                  "lang": "painless",
                  "source": "doc['start_time'].value.hourOfDay"
                }

            },
            "aggs" : {
              "aggs_keyword" : {
                "nested" : {
                  "path" : "neutral_word"
                },
                "aggs" : {
                  "top_keyword" : {
                    "terms" : {
                      "field": "neutral_word.word",
                      "size" : 15
                    }
                  }
                }
              }
            }
        }
    },

    "query":  {
        "nested" : {
            "path" : "neutral_word",
            "query" : {
                "bool" : {
                    "should" : [
                    { "term" : {"neutral_word.word": "색상"}},
                    { "term" : {"neutral_word.word": "배송"}}
                    
                    ]
                }
            },
            "score_mode" : "avg"
        }
    }

}

